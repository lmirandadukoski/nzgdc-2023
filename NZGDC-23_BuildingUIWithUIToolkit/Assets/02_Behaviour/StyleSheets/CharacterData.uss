CharacterDataVisualElement {
    flex-grow: 1;
    flex-direction: row;
    padding-top: 64px;
    justify-content: center;
}

CharacterDataVisualElement Label {
    flex-grow: 1;
    -unity-font-style: bold;
    font-size: 32px;
    color: rgb(210, 210, 210);
    -unity-text-align: middle-center;
    height: 40px;
    max-height: 40px;
    min-height: 40px;
    padding-top: 64px;
    padding-right: 0;
    margin-right: 0;
    padding-left: 0;
    width: 250px;
}
